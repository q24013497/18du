
@{
    ViewData["Title"] = "Index";
}
<script src="~/js/vue/vue.js"></script>
<script src="~/js/vue/axios.js"></script>
<h2>Index</h2>
<table id="Student">
    <tr >
        <td>ID</td>
        <td>商品名称</td>
        <td>市场价格</td>
        <td>会员价格</td>
        <td>销量</td>
        <td>剩余库存量</td>
        <td>品牌Id</td>
        <td>店铺Id</td>
        <td>所提供服务</td>
        <td>图片</td>
        <td>商品详情</td>
        <td>店铺类型</td>
        <td>创建时间</td>
    </tr>
    <tr v-stu="item in SSS">

        <td>{{item.id}}</td>
        <td>{{item.name}}</td>
        <td>{{item.marketPrice}}</td>
        <td>{{item.proPay}}</td>
        <td>{{item.sales}}</td>
        <td>{{item.sKU}}</td>
        <td>{{item.originId}}</td>
        <td>{{item.shopId}}</td>
        <td>{{item.saas}}</td>
        <td>{{item.image}}</td>
        <td>{{item.details}}</td>
        <td>{{item.storeType}}</td>
        <td>{{item.createTime}}</td>
    </tr>
</table id="Student">

<script>
    var vm = new Vue({
        el: "#Student",
        data: {
            SSS: []
        },
        mounted() {
            this.pagehandler();
        },
        methods: {
            pagehandler: function () {
                axios.get("http://localhost:53782/api/Mster1").then((response) => {
                    console.log(response)
                    this.SSS = [];
                    for (var i = 0; i < response.data.length; i++) {
                        this.SSS.push(response.data[i]);
                    }
                });
            }
        }


    });
</script>